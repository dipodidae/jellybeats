############################################################
# Jellybeats Environment Example
# Copy this file to .env (or .dev.development) and fill values.
# Never commit real secrets. Rotate any exposed keys immediately.
############################################################

# Base URL of your Jellyfin server (no trailing slash)
NUXT_JELLYFIN_URL=http://localhost:8096

# Jellyfin API key (server-side only). Use a low-scope key if possible.
NUXT_JELLYFIN_API_KEY=

# Primary Jellyfin user ID the app operates as (server usage)
NUXT_JELLYFIN_USER_ID=

# (Optional) Publicly exposable user id (only if required client-side). Leave blank to avoid exposure.
NUXT_PUBLIC_JELLYFIN_USER_ID=

# (Optional) Nuxt UI Pro license key â€“ remove if not using Pro features
NUXT_UI_PRO_LICENSE=

# Base URL again for nuxt-api-party endpoint mapping (often same as NUXT_JELLYFIN_URL)
NUXT_API_PARTY_ENDPOINTS_JELLYFIN_URL=http://localhost:8096

# Add additional custom variables below this line

